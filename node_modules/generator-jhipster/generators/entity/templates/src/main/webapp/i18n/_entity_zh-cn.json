{
    "<%= angularAppName %>": {
        "<%= entityTranslationKey %>" : {
            "home": {
                "title": "<%= entityName  %>",
                "createLabel": "新增<%= entityName %>",
                "createOrEditLabel": "新增或修改<%= entityName %>",
                "search": "搜索<%= entityName %>"
            },
            "created": "ID 为 {{ param }} 的<%= entityName  %>被创建",
            "updated": "ID 为 {{ param }} 的<%= entityName  %>被更新",
            "deleted": "ID 为 {{ param }} 的<%= entityName  %>被删除",
            "delete": {
                "question": "你确定要删除ID 为 {{ param }}<%= entityName %>?"
            },
            "detail": {
                "title": "<%= entityClassHumanized %>"
            }<% for (idx in fields) { %>,
            "<%=fields[idx].fieldName%>": "<%= fields[idx].fieldDescription %>"<% } %><% for (idx in relationships) { %>,
            "<%=relationships[idx].relationshipName%>": "<%= relationships[idx].fieldDescription %>"<% } %>
        }
    }
}
